    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.58.0",
